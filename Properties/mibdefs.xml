<?xml version="1.0" ?>
<!--
  This XML document is used to house the default object id definitions.  This is
  a way to define only the ones we would ever be interested in without the overhead
  of using a MIB browser.

  Using this method still keeps it flexible enough to add any oids we might be
  interested in as more SNMP enabled devices are encountered.
-->
<mib-defs>
  <!--   BEGIN A MIB DEFINITION
    A mib element represents a vendor or SNMP spec MIB.  Since we are only interested
    in the name and objectid that is all we keep for each object.  We get the object
    type at runtime.  Also note that we do not concern ourselves with all available
    objects here, but they can be added quite simply.
  -->
    <mib name="LISA Metrics">
      <object name="System Description"
              mib-name="lisaSysDescr"
              oid=".1.3.6.1.4.1.12841.1.1.0"
              path=""
              syntax="DisplayString  ( SIZE ( 1 .. 2048  ) ) "
              macrotype="OBJECT-TYPE">
        <description>The LISA application banner.</description>
      </object>

        <object name="System ObjectID"
                mib-name="lisaSysObjectID"
                oid=".1.3.6.1.4.1.12841.1.2.0"
                path=""
                syntax="DisplayString  ( SIZE ( 1 .. 2048  ) ) "
                macrotype="OBJECT-TYPE">
          <description>The LISA application name, which is either LISA Registry or a Coordinator Server.</description>
        </object>

        <object name="LISA Application Uptime"
                mib-name="sysUpTime"
                oid=".1.3.6.1.4.1.12841.1.3.0"
                path=""
                syntax="INTEGER  ( 0 .. 2147483647  ) "
                macrotype="OBJECT-TYPE">
          <description>LISA Server uptime in milliseconds.</description>
        </object>

        <object name="System Contact"
                mib-name="lisaSysContact"
                oid=".1.3.6.1.4.1.12841.1.4.0"
                path=""
                syntax="DisplayString  ( SIZE ( 1 .. 2048  ) ) "
                macrotype="OBJECT-TYPE">
          <description>This is the license holder of this LISA Server.</description>
        </object>

        <object name="System Name"
                mib-name="lisaSysName"
                oid=".1.3.6.1.4.1.12841.1.5.0"
                path=""
                syntax="DisplayString  ( SIZE ( 1 .. 2048  ) ) "
                macrotype="OBJECT-TYPE">
          <description>The LISA application name, which is either LISA Registry or a Coordinator Server.</description>
        </object>

        <object name="System Location"
                mib-name="lisaSysLocation"
                oid=".1.3.6.1.4.1.12841.1.6.0"
                path=""
                syntax="DisplayString  ( SIZE ( 1 .. 2048  ) ) "
                macrotype="OBJECT-TYPE">
          <description>This is the host name upon which this LISA Server is running.</description>
        </object>


        <!-- Tests table TOTALS Row -->
        <object name="Tests Table - Totals Row Number"
                mib-name="lisaTestTableTotalsRowNum"
                oid=".1.3.6.1.4.1.12841.2.2.1.1.1"
                path=""
                syntax="INTEGER  ( 0 .. 2147483647  )"
                macrotype="OBJECT-TYPE">
          <description>The summary information on all running tests -- this is the row number (always 1).</description>
        </object>


        <object name="Tests Table - Totals Test name"
                mib-name="lisaTestTableTotalsTestName"
                oid=".1.3.6.1.4.1.12841.2.2.1.2.1"
                path=""
                syntax="DisplayString  ( SIZE ( 1 .. 2048  ) ) "
                macrotype="OBJECT-TYPE">
          <description>The summary information on all running tests -- this is the test name (__Total).</description>
        </object>

        <object name="Tests Table - Total Instances"
                mib-name="lisaTestTableTotalsTestName"
                oid=".1.3.6.1.4.1.12841.2.2.1.3.1"
                path=""
                syntax="Guage"
                macrotype="OBJECT-TYPE">
          <description>This is the total number of instances running on all tests currently staged by Coordinators in this LISA Server.</description>
        </object>

        <object name="Tests Table - Minimum Node Execution Time"
                mib-name="lisaTestTableTotalsTestName"
                oid=".1.3.6.1.4.1.12841.2.2.1.4.1"
                path=""
                syntax="Guage"
                macrotype="OBJECT-TYPE">
          <description>This is the minimum observed execution time across all tests.</description>
        </object>

        <object name="Tests Table - Maximum Node Execution Time"
                mib-name="lisaTestTableTotalsTestName"
                oid=".1.3.6.1.4.1.12841.2.2.1.5.1"
                path=""
                syntax="Guage"
                macrotype="OBJECT-TYPE">
          <description>This is the maxiumum observed execution time across all tests.</description>
        </object>

        <object name="Tests Table - Average Execution Time"
                mib-name="lisaTestTableTotalsTestName"
                oid=".1.3.6.1.4.1.12841.2.2.1.6.1"
                path=""
                syntax="Guage"
                macrotype="OBJECT-TYPE">
          <description>This is the average observed execution time across all tests.</description>
        </object>

        <object name="Tests Table - Last Node Execution Time"
                mib-name="lisaTestTableTotalsTestName"
                oid=".1.3.6.1.4.1.12841.2.2.1.7.1"
                path=""
                syntax="Guage"
                macrotype="OBJECT-TYPE">
          <description>This is the last observed execution time across all tests.</description>
        </object>

    </mib>
    <!--   END OF A MIB DEFINITION -->


  <mib name="Server O/S">
    <!--
      An object inside a MIB.  We use this to build selection lists in a human readable form
      and use the oid to retrieve the data from the host running the needed data.
    -->
    <object name="hrSystemUptime"
            mib-name="hrSystemUptime"
            oid=".1.3.6.1.2.1.25.1.1"
            path=".iso.org.dod.internet.mgmt.mib-2.host.hrSystem.hrSystemUptime"
            syntax="TimeTicks"
            macrotype="OBJECT-TYPE">
      <description>The amount of time since this host was last initialized.  Note that this is different from sysUpTime in MIB-II [3] because sysUpTime is the uptime of the network management portion of the system.</description>
    </object>

    <object name="hrSystemDate"
            mib-name="hrSystemDate"
            oid=".1.3.6.1.2.1.25.1.2"
            path=".iso.org.dod.internet.mgmt.mib-2.host.hrSystem.hrSystemDate"
            syntax="DateAndTime"
            macrotype="OBJECT-TYPE">
      <description>The host's notion of the local date and time of day.</description>
    </object>

    <object name="hrSystemNumUsers"
            mib-name="hrSystemNumUsers"
            oid=".1.3.6.1.2.1.25.1.5"
            path=".iso.org.dod.internet.mgmt.mib-2.host.hrSystem.hrSystemNumUsers"
            syntax="Gauge"
            macrotype="OBJECT-TYPE">
      <description>The number of user sessions for which this host is storing state information. A session is a collection of processes requiring a single act of user authentication and possibly subject to collective job control.</description>
    </object>

    <object name="hrSystemProcesses"
            mib-name="hrSystemProcesses"
            oid=".1.3.6.1.2.1.25.1.6"
            path=".iso.org.dod.internet.mgmt.mib-2.host.hrSystem.hrSystemProcesses"
            syntax="Gauge"
            macrotype="OBJECT-TYPE">
      <description>The number of process contexts currently loaded or running on this system.</description>
    </object>

    <object name="hrSystemMaxProcesses"
            mib-name="hrSystemMaxProcesses"
            oid=".1.3.6.1.2.1.25.1.7"
            path=".iso.org.dod.internet.mgmt.mib-2.host.hrSystem.hrSystemMaxProcesses"
            syntax="INTEGER  ( 0 .. 2147483647  ) "
            macrotype="OBJECT-TYPE">
      <description>The maximum number of process contexts this system can support. If there is no fixed maximum, the value should be zero. On systems that have a fixed maximum, this object can help diagnose failures that occur when this maximum is reached.</description>
    </object>

    <object name="hrMemorySize"
            mib-name="hrMemorySize"
            oid=".1.3.6.1.2.1.25.2.2"
            path=".iso.org.dod.internet.mgmt.mib-2.host.hrStorage.hrMemorySize"
            syntax="KBytes"
            macrotype="OBJECT-TYPE">
      <description>The amount of physical main memory contained by the host.</description>
    </object>

    <object name="hrStorageSize"
            mib-name="hrStorageSize"
            oid=".1.3.6.1.2.1.25.2.3.1.5"
            path=".iso.org.dod.internet.mgmt.mib-2.host.hrStorage.hrStorageTable.hrStorageEntry.hrStorageSize"
            syntax="INTEGER  ( 0 .. 2147483647  )"
            macrotype="OBJECT-TYPE">
      <description>The size of the storage represented by this entry, in units of hrStorageAllocationUnits.</description>
    </object>

    <object name="hrProcessorLoad"
            mib-name="hrProcessorLoad"
            oid=".1.3.6.1.2.1.25.3.3.1.2"
            path=".iso.org.dod.internet.mgmt.mib-2.host.hrDevice.hrProcessorTable.hrProcessorEntry.hrProcessorLoad"
            syntax="INTEGER  ( 0 .. 100  )"
            macrotype="OBJECT-TYPE">
      <description>The average, over the last minute, of the percentage of time that this processor was not idle.</description>
    </object>

  </mib>
  <!--   END OF A MIB DEFINITION -->

