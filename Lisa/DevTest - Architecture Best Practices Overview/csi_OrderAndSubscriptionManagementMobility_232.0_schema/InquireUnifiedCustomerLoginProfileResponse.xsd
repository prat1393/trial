                          <xs:annotation>
                            <xs:documentation>Identifies if SLID access ID is autogenereated by MPS for Titanized Wireless accounts</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="accessIDStatusIndicator" type="xs:boolean">
                          <xs:annotation>
                            <xs:documentation>Indicates if an MPS autogenerated SLID has been activated by the customer</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="digitalLifeID" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>Digital Life Account ID</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="xanbooID" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>Xanboo Service ID</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="migrationType" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>Possible values may include but not enumerated
&quot;U&quot; - unseen (but migrated), 
&quot;M&quot; - migrated,
&quot;Y&quot; - ISP and DSL migration</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="migrationDate" type="cdm:DateTimeWithTimezoneInfo">
