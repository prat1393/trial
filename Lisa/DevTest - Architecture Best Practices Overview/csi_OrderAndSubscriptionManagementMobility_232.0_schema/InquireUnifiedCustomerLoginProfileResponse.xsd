                    <xs:annotation>
                      <xs:documentation>Whether or not to use this account for default CPNI notification addresses</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="accountNickname" type="xs:string"/>
                  <xs:element minOccurs="0" name="lastVerifiedPassCode" type="cdm:SecureEncryptedGenericInfo"/>
                  <xs:element minOccurs="0" name="accountPassCodeEstablishedDate" type="cdm:DateWithTimezoneInfo"/>
                  <xs:element minOccurs="0" name="accountAssociationCreateTime" type="cdm:DateWithTimezoneInfo"/>
