                      <xs:sequence>
                        <xs:element minOccurs="0" name="emailAddress" type="cdm:EmailAddressInfo"/>
                        <xs:element minOccurs="0" name="emailStatus" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>Indicate the eamil status</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="emailAddressEstablishedDate" type="cdm:DateWithTimezoneInfo"/>
                        <xs:element minOccurs="0" name="emailValidIndicator" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>Indicate contact email address verified</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="associationIdentifier" type="xs:string"/>
                        <xs:element minOccurs="0" name="contactAssociationTUID">
                          <xs:annotation>
                            <xs:documentation>			</xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:maxLength value="22"/>
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:element>
                        <xs:element minOccurs="0" name="contactType">
                          <xs:annotation>
                            <xs:documentation>			</xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:maxLength value="30"/>
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:element>
                        <xs:element minOccurs="0" name="phoneNumber" type="cdm:TelephoneNumberInfo">
                          <xs:annotation>
                            <xs:documentation>Can Be Reached Telephone Number</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="phoneNumberType">
                          <xs:annotation>
                            <xs:documentation>Can Be Reached Telephone Number Type</xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:maxLength value="10"/>
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:element>
                        <xs:element minOccurs="0" name="validationStatusDate" type="cdm:DateTimeWithTimezoneInfo">
                          <xs:annotation>
                            <xs:documentation>Contact Validation Status Timestamp</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="phoneExt">
                          <xs:annotation>
                            <xs:documentation>Phone Number Extension</xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:maxLength value="20"/>
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:element>
                        <xs:element minOccurs="0" name="contactTUID">
                          <xs:annotation>
                            <xs:documentation>			</xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:maxLength value="22"/>
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:element>
                        <xs:element minOccurs="0" name="simSwapDateTime" type="cdm:DateTimeWithTimezoneInfo"/>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="UserIdentity">
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="memberNumber" type="cdm:MemberNumberInfo"/>
                        <xs:element minOccurs="0" name="memberIdentifier" type="cdm:SLIDMemberIdInfo">
                          <xs:annotation>
                            <xs:documentation>Email/Telephone format to indicate a member</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="memberName" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>Handle portion of member ID</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="identityTypeName" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>Indicate the type of identity such as memberID or CTN etc..</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="offlineSecurityQuestionCode" type="xs:int">
                          <xs:annotation>
                            <xs:documentation>Identifies the code for the offline security question</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="offlineSecurityAnswerText" type="cdm:SecureEncryptedGenericInfo">
                          <xs:annotation>
                            <xs:documentation>The answer for offline Security Question </xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="identityPassCode" type="cdm:SecureEncryptedGenericInfo">
                          <xs:annotation>
                            <xs:documentation>Passcode attached to identity
</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="identityProofingZipCode" type="cdm:AddressZipCodeInfo">
                          <xs:annotation>
                            <xs:documentation>Zip code used for proofing</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="securityQuestion1Code" type="xs:int">
                          <xs:annotation>
                            <xs:documentation>Identifies the code for Online security question 1</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="securityQuestion2Code" type="xs:int">
                          <xs:annotation>
                            <xs:documentation>Identifies the code for Online security question 2</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="portalBrowserLanguage" type="cdm:LanguageInfo">
                          <xs:annotation>
                            <xs:documentation>2 Characters Portal Browser language preference</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="myATTBrowserLanguage" type="cdm:LanguageInfo">
                          <xs:annotation>
                            <xs:documentation>2 Characters Portal Browser language preference</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="csrInitialtedLockoutDate" type="cdm:DateWithTimezoneInfo">
                          <xs:annotation>
                            <xs:documentation>Customer Service Respresentative initiated lock Date</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="identifyStatusCode" type="xs:int">
                          <xs:annotation>
                            <xs:documentation>The last known member status</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="designatedMainSubscriberNumber" type="cdm:TelephoneNumberInfo">
                          <xs:annotation>
                            <xs:documentation>Main CTN</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="designatedMemberIdentifier" type="cdm:FiberServiceMemberIdInfo">
                          <xs:annotation>
                            <xs:documentation>Main UversID</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="subscriberIdentifer" type="cdm:SubscriberIdInfo">
                          <xs:annotation>
                            <xs:documentation>Wireless Subscriber ID - subID</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="iptvTelegenceIdentifier" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>User IPTV ciructID for service access</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="mainCTNOptOutDateTime" type="cdm:DateTimeWithTimezoneInfo">
                          <xs:annotation>
                            <xs:documentation>the date on which  the customer  selected not to be &apos;nagged&apos; to create a Designated Main CTN.  GMT format</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="interceptCode" type="xs:integer">
                          <xs:annotation>
                            <xs:documentation>Source DB Column: mps.customer_intercept_list.interceptcode. interceptCode value is 1 ,0 Missing/Conflict (?0010?=2), Link Recommender and Missing/Conflict (?0011?=3)</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="questionAndAnswerLastUpdatedDateTime" type="cdm:DateTimeWithTimezoneInfo"/>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="DTVAssociation">
              <xs:annotation>
                <xs:documentation>returned for Level 2 and Level 3 account associations</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element minOccurs="0" name="memberNumber" type="xs:string">
                    <xs:annotation>
                      <xs:documentation>DTV member number</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="deviceIdentifier" type="xs:string"/>
                  <xs:element minOccurs="0" name="accountNumber">
                    <xs:annotation>
                      <xs:documentation>DTV account number</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:maxLength value="1024"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:element minOccurs="0" name="IntegratedOfferAccountDetails" type="cdm:IntegratedOfferAccountDetails">
                    <xs:annotation>
                      <xs:documentation>IntegratedOfferAccountDetails details return for  level3 billingAccount asscociation</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element minOccurs="0" name="UserFraudAssociation">
              <xs:annotation>
                <xs:documentation>user level Fraud Attributes. Returned For Level 3 Account Association. </xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element minOccurs="0" name="memberNumber" type="cdm:MemberNumberInfo">
                    <xs:annotation>
                      <xs:documentation>The ID used to identify the user, aka GUID</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="lockLevel">
                    <xs:annotation>
                      <xs:documentation>Valid Values 1 or 2</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:enumeration value="1"/>
                        <xs:enumeration value="2"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:element minOccurs="0" name="fraudDateTime" type="cdm:DateTimeWithTimezoneInfo">
                    <xs:annotation>
                      <xs:documentation>Fraud date time when created</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="updatedFraudDateTime" type="cdm:DateTimeWithTimezoneInfo">
                    <xs:annotation>
                      <xs:documentation>Updated(last) Fraud date time.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="reasonCode">
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:maxLength value="50"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:element minOccurs="0" name="initiatingSystem">
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:maxLength value="50"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
