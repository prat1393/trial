                          <xs:annotation>
                            <xs:documentation>User Birth Date Expects YYYY-MM-DD format</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="userGenderCode" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>Member&apos;s gender</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="memberNumberCreationDate" type="cdm:DateWithTimezoneInfo"/>
                        <xs:element minOccurs="0" name="last4SSN" type="cdm:SocialSecurityNumberLastFourDigitsInfo"/>
                        <xs:element minOccurs="0" name="creditCheckedInd">
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:length value="1"/>
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:element>
                        <xs:element minOccurs="0" name="cloudConsentIndicator" type="cdm:BooleanTrueInfo">
                          <xs:annotation>
                            <xs:documentation>Indicate user having cloud service consent</xs:documentation>
                          </xs:annotation>
                        </xs:element>
