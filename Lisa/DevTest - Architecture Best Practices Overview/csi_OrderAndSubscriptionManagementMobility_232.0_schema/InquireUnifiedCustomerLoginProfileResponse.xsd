<xs:schema elementFormDefault="qualified" targetNamespace="http://csi.cingular.com/CSI/Namespaces/Container/Public/InquireUnifiedCustomerLoginProfileResponse.xsd" version="232.0.00" xmlns="http://csi.cingular.com/CSI/Namespaces/Container/Public/InquireUnifiedCustomerLoginProfileResponse.xsd" xmlns:cdm="http://csi.att.com/CSI/Namespaces/OrderAndSubscriptionManagementMobility/Types/Public/CommonDataModel.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:import namespace="http://csi.att.com/CSI/Namespaces/OrderAndSubscriptionManagementMobility/Types/Public/CommonDataModel.xsd" schemaLocation="CommonDataModel.xsd"/>
  <xs:element name="InquireUnifiedCustomerLoginProfileResponse" type="InquireUnifiedCustomerLoginProfileResponseInfo">
    <xs:annotation>
      <xs:documentation>Retrieves the customer profile information for Universal Household View (UHV)</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="InquireUnifiedCustomerLoginProfileResponseInfo">
    <xs:sequence>
      <xs:element maxOccurs="unbounded" name="CustomerLoginProfile">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" name="UserIdentityContact">
