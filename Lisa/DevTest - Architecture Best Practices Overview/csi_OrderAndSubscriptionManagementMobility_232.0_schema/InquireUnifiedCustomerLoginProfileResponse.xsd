                <xs:documentation>User, Identity and Contact Data</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element minOccurs="0" name="User">
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="memberNumber" type="cdm:MemberNumberInfo">
                          <xs:annotation>
                            <xs:documentation>The ID used to identify the user, aka GUID</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="UserName" type="cdm:NameInfo"/>
                        <xs:element minOccurs="0" name="userNickname" type="xs:string"/>
                        <xs:element minOccurs="0" name="parentChildCode">
                          <xs:annotation>
                            <xs:documentation>Identifies the member as a parent, child or email alias or SLID.  Note: ?S? does not stand for subaccount ? It stands for slid.  Subaccounts are ?C? for child.</xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:enumeration value="P"/>
                              <xs:enumeration value="C"/>
                              <xs:enumeration value="S"/>
                              <xs:enumeration value="E"/>
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:element>
                        <xs:element minOccurs="0" name="parentMemberIdentifier" type="cdm:SLIDMemberIdInfo">
                          <xs:annotation>
                            <xs:documentation>The parent memberID or CTN</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="emailAliasMainMemberNumber" type="cdm:MemberNumberInfo">
                          <xs:annotation>
                            <xs:documentation>Identifies the associated member ID when ParentChildcode is &quot;E&quot;</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="slidMemberNumber" type="cdm:MemberNumberInfo">
                          <xs:annotation>
                            <xs:documentation>Identifies the SLID that a member ID is linked to. 
</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element minOccurs="0" name="userAccountTypeCode" type="xs:string">
