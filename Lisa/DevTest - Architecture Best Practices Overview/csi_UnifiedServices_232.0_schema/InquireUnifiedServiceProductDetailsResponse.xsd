<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://csi.cingular.com/CSI/Namespaces/Container/Public/InquireUnifiedServiceProductDetailsResponse.xsd" version="232.0.00" xmlns="http://csi.cingular.com/CSI/Namespaces/Container/Public/InquireUnifiedServiceProductDetailsResponse.xsd" xmlns:cdm="http://csi.att.com/CSI/Namespaces/UnifiedServices/Types/Public/CommonDataModel.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema">
